{
    "CG_params": {
        "num_hiddens": 1,
        "ll": [
            8,
            8
        ],
        "conv_activation": "sigmoid",
        "h_embed": true,
        "use_probs": false,
        "use_logits": true,
        "relaxation_rate": 0.0005,
        "min_temperature": 0.1,
        "init_temperature": 0.65
    },

    "critic_params": {
        "layers": 3,
        "embed_dim": 8,
        "hidden_dim": 32,
        "activation": "relu",
        "use_dropout": true,
        "dropout_rate": 0.25
    },

    "opt_params": {
        "batch_size": 500,
        "iterations": 200,
        "shuffle": 30000,
        "learning_rate": 4e-3
    }
}
